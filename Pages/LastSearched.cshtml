@page
@model proj.Pages.LastSearchedModel;
@{
    ViewData["Title"] = "Data in database";
}

<p class="savedTitle">Ostatnio szukane:</p>

@if(Model.people.FirstOrDefault() != null){

    <div>

        @foreach (var item in Model.people)
        {
            <div class="myRow">
                <p>
                @item.Name , @item.LastName , @item.Year -&nbsp; 
                </p>@if(item.Year % 4 == 0){
                    <p>rok przestępny</p>
                }else{
                    <p>rok nieprzestępny</p>
                }
            </div>
        }

    </div>
}